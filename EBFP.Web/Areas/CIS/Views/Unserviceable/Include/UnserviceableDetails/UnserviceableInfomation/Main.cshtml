@model EBFP.BL.CIS.UnserviceableModel
@using EBFP.BL.Helper
<style>

</style>

<div class="col-sm-12">
  <div class="panel panel-info">
    <a href="#" data-perform="panel-collapse">
      <div class="panel-heading">
        Unserviceable Physical Inventory Details
        <div class="pull-left">
          <a href="#" data-perform="panel-collapse"><i class="ti-minus"></i>&nbsp;&nbsp;</a>
        </div>
      </div>
    </a>
    <div class="panel-wrapper collapse in" aria-expanded="true">
      <div class="panel-body">
        <div class="row">
          <div class="col-sm-6 col-xs-12">
            <div class="form-horizontal">

              <div class="form-group">
                <label class="col-md-3 control-label">WMR</label>
                <div class="col-md-6">
                  @Html.TextBoxFor(model => model.UPI_WMR, new {@class = "form-control"})
                </div>
              </div>

              <div class="form-group">
                <label class="col-md-3 control-label">Reporting Office</label>
                <div class="col-md-6">
                  @Html.TextBoxFor(model => model.UPI_ReportingOffice, new {@class = "form-control"})
                </div>
              </div>                
            </div>      
          </div>
          <div class="form-group">
            @Html.Partial("~/Areas/CIS/Views/Unserviceable/Include/UnserviceableDetails/PhysicalInventory/Main.cshtml", @Model)
          </div>
        </div>
        <div>
          <div class="form-group">
          </div>
        </div>
      </div>

    </div>

  </div>
</div>