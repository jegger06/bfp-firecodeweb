
@model EBFP.BL.HumanResources.PhysicalExamModel



<div class="col-md-12 chk-label">
    <div class="chk-group">
        @Html.CheckBoxFor(model => model.ChestBreast.PE_ChestGrossly, new { @class = "form-control" })
        <label class="control-label chk-label">Chest Grossly Symmetrical Bilaterally</label>
    </div>
    <div class="chk-group">
        @Html.CheckBoxFor(model => model.ChestBreast.PE_BreastExam_Deferred, new { @class = "form-control" })
        <label class="control-label chk-label">Breast Exam Deferred</label>
    </div>
    <div class="chk-group">
        @Html.CheckBoxFor(model => model.ChestBreast.PE_No_Breast_Dippling, new { @class = "form-control" })
        <label class="control-label chk-label">No Breast Dippling</label>
    </div>
    <div class="chk-group">
        @Html.CheckBoxFor(model => model.ChestBreast.PE_No_Drainage, new { @class = "form-control" })
        <label class="control-label chk-label">No Drainage</label>
    </div>
</div>
<div class="col-md-12" style="padding-left: 5px">
    <div class="chk-group">
        @Html.CheckBoxFor(model => model.ChestBreast.PE_No_Breast_Masses, new { @class = "form-control" })
        <label class="control-label chk-label">No Breast Masses</label>
    </div>
    <div class="chk-group">
        @Html.CheckBoxFor(model => model.ChestBreast.PE_No_Chest_Breast_Nodules, new { @class = "form-control" })
        <label class="control-label chk-label">No Chest or Breast Nodules</label>
    </div>
    <div class="chk-group">
        @Html.CheckBoxFor(model => model.ChestBreast.PE_No_Nipple_Inversion, new { @class = "form-control" })
        <label class="control-label chk-label">No Nipple Inversion</label>
    </div>
    <div class="chk-group">
        @Html.CheckBoxFor(model => model.ChestBreast.PE_No_Axillary_Nodes_Bilaterally, new { @class = "form-control" })
        <label class="control-label chk-label">No Axillary Nodes Bilaterally</label>
    </div>
</div>
<div class="col-md-12" style="padding-left: 5px">
    <div class="chk-group">
        @Html.CheckBoxFor(model => model.ChestBreast.PE_Breast_Absent, new { @class = "form-control" })
        <label class="control-label chk-label">Breast(s) Absent: </label>
    </div>
    <div class="chk-group">
        @Html.CheckBoxFor(model => model.ChestBreast.PE_Breast_Absent_Right, new { @class = "form-control" })
        <label class="control-label chk-label">Right</label>
    </div>
    <div class="chk-group" style="padding-right: 20px;">
        @Html.CheckBoxFor(model => model.ChestBreast.PE_Breast_Absent_Left, new { @class = "form-control" })
        <label class="control-label chk-label">Left</label>
    </div>
    <div class="chk-group">
        @Html.CheckBoxFor(model => model.ChestBreast.PE_Breast_Implant, new { @class = "form-control" })
        <label class="control-label chk-label">Implant Status</label>
    </div>
    <div class="chk-group">
        @Html.CheckBoxFor(model => model.ChestBreast.PE_Breast_Implant_Right, new { @class = "form-control" })
        <label class="control-label chk-label">Right</label>
    </div>
    <div class="chk-group" style="padding-right: 20px;">
        @Html.CheckBoxFor(model => model.ChestBreast.PE_Breast_Implant_Left, new { @class = "form-control" })
        <label class="control-label chk-label">Left</label>
    </div>
    <div class="chk-group">
        @Html.CheckBoxFor(model => model.ChestBreast.PE_Cosmetic, new { @class = "form-control" })
        <label class="control-label chk-label">Cosmetic</label>
    </div>
    <div class="chk-group">
        @Html.CheckBoxFor(model => model.ChestBreast.PE_Reconstructive, new { @class = "form-control" })
        <label class="control-label chk-label">Reconstructive</label>
    </div>
</div>
<div class="col-md-12" style="padding-top: 5px;">
    <div class="col-md-3">
        <label class="control-label chk-label">Abnormal Exam Findings : </label>
    </div>
    <div class="col-md-9">
        @Html.TextBoxFor(model => model.ChestBreast.PE_Chest_Breast_Abnormal_Findings, new { @class = "form-control" })
    </div>
</div>