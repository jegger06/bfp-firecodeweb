
@model EBFP.BL.HumanResources.PhysicalExamModel



<div class="col-md-12" style="padding-left: 5px">
    <div class="chk-group">
        @Html.CheckBoxFor(model => model.Psych.PE_Mood_Affect, new { @class = "form-control" })
        <label class="control-label chk-label">Mood and Affect</label>
    </div>
    <div class="col-md-1">
        @Html.TextBoxFor(model => model.Psych.PE_Nomral_Text, new { @class = "form-control" })
    </div>
    <div class="chk-group">
        <label class="control-label chk-label"> Normal </label>
    </div>
    <div class="col-md-1">
        @Html.TextBoxFor(model => model.Psych.PE_Depressed_Text, new { @class = "form-control" })
    </div>
    <div class="chk-group">
        <label class="control-label chk-label"> Depressed </label>
    </div>
    <div class="col-md-1">
        @Html.TextBoxFor(model => model.Psych.PE_Anxiuos_Text, new { @class = "form-control" })
    </div>
    <div class="chk-group">
        <label class="control-label chk-label"> Anxious </label>
    </div>
    <div class="col-md-1">
        @Html.TextBoxFor(model => model.Psych.PE_Agitated_Text, new { @class = "form-control" })
    </div>
    <div class="chk-group">
        <label class="control-label chk-label"> Agitated </label>
    </div>
</div>
<div class="col-md-12" style="padding-top: 5px">
    <div class="col-md-3">
        <label class="control-label chk-label">Abnormal Exam Findings: </label>
    </div>
    <div class="col-md-9">
        @Html.TextBoxFor(model => model.Psych.PE_Psych_Abnormal_Findings, new { @class = "form-control" })
    </div>
</div>