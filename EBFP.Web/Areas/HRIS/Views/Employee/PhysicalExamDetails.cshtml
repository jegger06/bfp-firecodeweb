                                </tr>
                                <tr>
                                    <td>
                                        @Html.CheckBoxFor(model => model.Deferred.PE_Neck_Deferred, new { @style = "float:none" })
                                    </td>
                                    <td style="width: 100px;">
                                        NECK
                                    </td>
                                    <td>
                                        @Html.Partial("~/Areas/HRIS/Views/Employee/Include/MedicalDetails/PhysicalExam/Partial/Neck.cshtml")
                                    </td>

                                </tr>
                                <tr>
                                    <td>
                                        @Html.CheckBoxFor(model => model.Deferred.PE_Respiratory_Deferred, new { @style = "float:none" })
                                    </td>
                                    <td style="width: 100px;">
                                        RESPIRATORY
