                                    </td>
                                    <td style="width: 100px;">
                                        ENT
                                    </td>
                                    <td>
                                        @Html.Partial("~/Areas/HRIS/Views/Employee/Include/MedicalDetails/PhysicalExam/Partial/ENT.cshtml")
