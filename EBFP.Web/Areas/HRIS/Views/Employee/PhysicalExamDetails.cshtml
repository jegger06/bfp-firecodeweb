                                <tr>
                                    <td style="width: 100px;">
                                        If System deferred, check here
                                    </td>
                                    <td colspan="2">
                                        (Please complete thoroughly each section unless exam component was deferred - All abnormals MUST be documented)
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        @Html.CheckBoxFor(model => model.Deferred.PE_General_Deferred, new { @style = "float:none" })
                                    </td>
                                    <td style="width: 100px;">
                                        GENERAL
                                    </td>
                                    <td>
                                        @Html.Partial("~/Areas/HRIS/Views/Employee/Include/MedicalDetails/PhysicalExam/Partial/General.cshtml")
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        @Html.CheckBoxFor(model => model.Deferred.PE_Head_Deferred, new { @style = "float:none" })
                                    </td>
                                    <td style="width: 100px;">
                                        HEAD
