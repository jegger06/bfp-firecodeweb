                                    <td>
                                        @Html.Partial("~/Areas/HRIS/Views/Employee/Include/MedicalDetails/PhysicalExam/Partial/Respiratory.cshtml")
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        @Html.CheckBoxFor(model => model.Deferred.PE_Cardiac_Deferred, new { @style = "float:none" })
                                    </td>
                                    <td style="width: 100px;">
                                        CARDIAC
