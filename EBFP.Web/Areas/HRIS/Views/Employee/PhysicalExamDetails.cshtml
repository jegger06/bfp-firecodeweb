                                </tr>
                                <tr>
                                    <td>
                                        @Html.CheckBoxFor(model => model.Deferred.PE_Eyes_Deferred, new { @style = "float:none" })
                                    </td>
                                    <td style="width: 100px;">
                                        EYES
                                    </td>
                                    <td>
